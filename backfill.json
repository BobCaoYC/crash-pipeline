{
  "mode": "backfill",
  "source": "crash",
  "join_key": "crash_record_id",
  "date_range": {
    "field": "crash_date",
    "start": "2018-06-20T00:00:00",
    "end":   "2018-06-21T00:00:00"
  },
  "primary": {
    "id": "85ca-t3if",
    "alias": "crashes",
    "order": "crash_date, crash_record_id, posted_speed_limit, traffic_control_device, device_condition, weather_condition, lighting_condition, trafficway_type, lane_cnt, alignment, roadway_surface_cond, road_defect, street_no, street_name, street_direction, crash_type",
    "page_size": 2000
  },
  "enrich": [
    {
      "id": "68nd-jvt3",
      "alias": "vehicles",
      "select": "crash_record_id,vehicle_id,make,model,travel_direction"
    },
    {
      "id": "u6pd-qa9d",
      "alias": "people",
      "select": "crash_record_id,person_id,person_type"
    }
  ],
  "batching": {
    "id_batch_size": 50,
    "max_workers": { "vehicles": 4, "people": 4 }
  },
  "storage": {
    "bucket": "raw-data",
    "prefix": "crash",
    "compress": true
  }
}